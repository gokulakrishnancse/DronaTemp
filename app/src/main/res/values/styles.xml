<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="backgroundcolor">#fcfcfc</item>
        <item name="cardbackgroundcolor">#ffff</item>
        <item name="textcolor">#000</item>
        <item name="iconcolor">#000</item>
        <item name="imagecolor">#FF000000</item>



    </style>
    <style name="darktheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#514c4c</item>
        <item name="colorPrimaryDark">#000000</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="backgroundcolor">#535050</item>
        <item name="cardbackgroundcolor">#080808</item>
        <item name="textcolor">#ffffff</item>
        <item name="iconcolor">#ffffff</item>
        <item name="imagecolor">#ffffff</item>
    </style>

</resources>
